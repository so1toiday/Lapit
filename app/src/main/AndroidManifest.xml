<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lapit">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:name=".ui.LapitChatAplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.acitivity.ChatActivity" android:parentActivityName=".ui.acitivity.MainActivity">
        </activity>
        <activity android:name=".ui.acitivity.ProfileActivity" />
        <activity
            android:name=".ui.acitivity.UsersActivity"
            android:parentActivityName=".ui.acitivity.MainActivity" />
        <activity android:name=".ui.acitivity.SettingActivity" />
        <activity android:name=".ui.acitivity.ResetPasswordActivity" />
        <activity android:name=".ui.acitivity.MainActivity" />
        <activity
            android:name=".ui.acitivity.RegisterActivity"
            android:parentActivityName=".ui.acitivity.StartActivity" />
        <activity android:name=".ui.acitivity.LoginActivity" />
        <activity android:name=".ui.acitivity.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>